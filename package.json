{
  "name": "intesys-pcto-fe-2023",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/intesys/intesys-pcto-fe-2023.git"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel src/index.html --port 3100",
    "build": "parcel build src/index.html",
    "test": "jest",
    "api:generate": "openapi-generator-cli generate --enable-post-process-file -i api.yml -t src/generated/templates/ -g typescript-axios -o src/generated/axios/",
    "api:version": "node ./scripts/api-version.js",
    "build:api": "npm run api:generate && npm run api:version",
    "postbuild": "npm run msw:install",
    "prestart": "npm run msw:install",
    "msw:install": "msw init dist --save"
  },
  "author": "Alessandro Falezza",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.15",
    "axios": "^0.27.2",
    "jest": "^29.5.0",
    "moment": "^2.29.4",
    "parcel": "^2.8.3",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "sass": "^1.60.0",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@parcel/config-default": "^2.8.3",
    "@parcel/resolver-glob": "^2.8.3",
    "@parcel/transformer-raw": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/sass": "^1.43.1",
    "msw": "^1.2.1",
    "process": "^0.11.10"
  },
  "msw": {
    "workerDirectory": "dist"
  }
}
